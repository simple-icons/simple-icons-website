      ul.grid
        //- This script adds an extra element before itself:
        script#_carbonads_js(
          async,
          src='//cdn.carbonads.com/carbon.js?serve=CKYIPK7M&placement=simpleiconsorg',
          type='text/javascript'
        )
        each icon in icons
          //- Using `o="{order-alpha}-{order-color}"` attribute to store ordering data
          //- for sorting in order to reduce the size of the DOM.
          //- Using `a="{aliases}"` attribute to store aliases data for searching
          //- Using `d="{deprecated}"` attribute to store deprecated data
          //- Using `s="{source}"` attribute to store source from which the icon was taken
          //- Using `lt="{license-type}"` attribute to store license type
          //- Using `lu="{license-url}"` attribute to store license url
          //- Using `g="{guidelines}"` attribute to store icon guidelines url
          li.grid-item(
            o=`${icon.indexByAlpha}-${icon.indexByColor}`,
            a=icon.plainAliases,
            d=icon.deprecatedAt === false ? false : JSON.stringify(icon.deprecatedAt),
            s=icon.source,
            lt=icon.license ? icon.license.type : false,
            lu=icon.license ? icon.license.url : false,
            g=icon.guidelines || false
          )
            .grid-item__row.mv-2.fg-2
              button.grid-item__preview.copy-button.copy-svg(
                title=`${ t_('$iconTitle SVG').replace('$iconTitle', icon.localizedTitle || icon.title) }`,
                disabled
              )
                //- The images are lazy loaded via Javascript with an IntersectionObserver
                //- using loading=lazy as fallback for browsers that don't support it.
                img.icon-preview(d-src=`/icons/${icon.slug}.svg`)
            if icon.deprecatedAt
              span.deprecated
                span.deprecated__text #{ t_('Deprecated') }
                a.deprecated__message(
                  target='_blank',
                  href=`https://github.com/simple-icons/simple-icons/milestone/${icon.deprecatedAt.milestoneNumber}`
                ) #{ t_('Removal at $version').replace('$version', `v${icon.deprecatedAt.version}`) }
            .grid-item__row
              if icon.guidelines
                a.grid-item__link.link-button(
                  title=`${ t_('$iconTitle guidelines').replace('$iconTitle', icon.localizedTitle || icon.title) }`,
                  href=icon.guidelines,
                  rel='noopener',
                  target='_blank'
                ) #{ t_('Brand Guidelines') }
              if icon.license
                a.grid-item__link.link-button.icon-legal(
                  title=`${ t_('$iconTitle icon license').replace('$iconTitle', icon.localizedTitle || icon.title) }`,
                  href=icon.license.url,
                  rel='noopener',
                  target='_blank'
                ) #{ icon.license.type }
              h2.grid-item__title.copy-button.copy-slug #{ icon.localizedTitle || icon.title }
            .grid-item__footer
              button(
                class=`grid-item__color copy-button copy-color ${icon.light ? "contrast-light" : "contrast-dark"}`,
                style=`background: #${icon.shortHex}`,
                title=`${ t_('$iconTitle color').replace('$iconTitle', icon.localizedTitle || icon.title) }`,
                disabled
              ) ##{ icon.hex }
              button.grid-item__button.view-button(
                title=`${ t_('view $iconTitle').replace('$iconTitle', icon.title) }`,
                index=`${icon.indexByAlpha}`
              )
                svg(role='img', viewBox='0 0 24 24')
                  use(href='#vw')
              button.grid-item__button
                svg(role='img', viewBox='0 0 24 24')
                  use(href='#cp')
      p#icon-missing.report-link #{ t_('Icon missing?') } #[a(href='https://github.com/simple-icons/simple-icons/issues/new?assignees=&labels=new+icon&template=icon_request.yml') #{ t_('Submit a request') }]
      p#icon-outdated.report-link #{ t_('Icon outdated?') } #[a(href='https://github.com/simple-icons/simple-icons/issues/new?assignees=&labels=icon+outdated&template=icon_update.yml') #{ t_('Report outdated icon') }]
