{
  "name": "simple-icons-website",
  "version": "1.0.0",
  "description": "Website for Simple Icons",
  "private": "true",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/simple-icons/simple-icons.git"
  },
  "bugs": {
    "url": "https://github.com/simple-icons/simple-icons/issues"
  },
  "author": "Simple Icons Collaborators",
  "license": "CC0-1.0",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:watch": "npm run build:dev -- --watch",
    "clean": "rm -rf _site tests/_artifacts",
    "pretest": "npm run clean && npm run build:dev",
    "serve": "anywhere -p 8080 -d ./_site",
    "test": "jest"
  },
  "dependencies": {
    "simple-icons": "4.3.0",
    "simple-icons-pdf": "https://github.com/simple-icons/simple-icons-pdf/tarball/master"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "anywhere": "1.5.0",
    "autoprefixer": "10.2.1",
    "babel-jest": "26.6.3",
    "color-sorter": "4.0.1",
    "copy-webpack-plugin": "7.0.0",
    "css-loader": "5.0.1",
    "css-minimizer-webpack-plugin": "1.2.0",
    "get-relative-luminance": "1.0.0",
    "html-webpack-plugin": "5.0.0-beta.4",
    "jest": "26.6.3",
    "jest-puppeteer": "4.4.0",
    "mini-css-extract-plugin": "1.3.3",
    "postcss": "8.2.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "4.1.0",
    "pug": "3.0.0",
    "pug-loader": "2.4.0",
    "puppeteer": "5.5.0",
    "svg-url-loader": "7.1.1",
    "webpack": "5.12.2",
    "webpack-cli": "4.3.1"
  },
  "engine": {
    "node": ">=12.16.0"
  }
}
